<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition 
		name="base"
		template="/WEB-INF/layout/page.jsp">
		<put-attribute 	name="header"
				value="/WEB-INF/layout/header.jsp" />
		<put-attribute 	name="footer"
				value="/WEB-INF/layout/footer.jsp" />
	</definition>	
	<definition name="multiplexer" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/multiplexer.jsp" />
	</definition>		
	<definition name="springlab-main" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/main.jsp" />
	</definition>
	<definition name="presentation-main" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/presentation/main.jsp" />
	</definition>	
	<definition name="registerForm" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/registerForm.jsp" />
	</definition>	
	<definition name="profile" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/profile.jsp" />
	</definition>
	<definition name="spittles" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/spittles.jsp" />
	</definition>	
	<definition name="spittle" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/spittle.jsp" />
	</definition>
	
	<definition name="spittleForm" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/spittleForm.jsp" />
	</definition>	
		
	<definition name="login" extends="base">
		<put-attribute 	name="body"
				value="/WEB-INF/views/springlab/login.jsp" />
	</definition>
</tiles-definitions>
